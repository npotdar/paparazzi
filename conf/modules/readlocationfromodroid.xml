<!DOCTYPE module SYSTEM "module.dtd">

<module name="readlocationfromodroid">
  <doc>
    <description>Reads and writes to and from the odroid</description>
  </doc>
  <settings>
    <dl_settings>
      <dl_settings name="serialjson">
        <dl_setting var="resetOdroid" min="0" step="1" max="1" shortname="resetOdroid" module="readlocationfromodroid/readlocationfromodroid"/>
      </dl_settings>
    </dl_settings>
  </settings>
  
  <header>
    <file name="readlocationfromodroid.h"/>
  </header>
  <init fun="odroid_loc_init()"/>
  <periodic fun="odroid_loc_periodic()" freq="10" autorun="TRUE"/>
  <makefile>
    <file name="readlocationfromodroid.c"/>
    <file name="cJSON.c"/>
   <!-- <file name="subsystems/gps.c"/>
    <define name="USE_GPS"/>
    <define name="GPS_TYPE_H" value="modules/readlocationfromodroid/readlocationfromodroid.h"/>-->
	<!--<file name="ivy.c"/>-->
  </makefile>
</module>

